generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model SavedQuote {
  id        Int      @id @default(autoincrement())
  userId    String
  author    String
  text      String
  tags      String   // comma-separated; can be empty string
  createdAt DateTime @default(now())
}
